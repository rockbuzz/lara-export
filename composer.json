{
  "name": "rockbuzz/lara-export",
  "description": "Laravel Export",
  "keywords": ["laravel", "export"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tiago Lopes",
      "email": "tiago@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraExport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "yidas/phpspreadsheet-helper": "^1.3"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraExport\\ServiceProvider"
      ]
    }
  }
}
